{
    "openapi": "3.0.0",
    "info": {
        "title": "API文档",
        "description": "公共接口",
        "contact": {
            "name": "API Develop Team"
        },
        "version": "1.0"
    },
    "servers": [
        {
            "url": "/api/common/",
            "description": "开发环境"
        }
    ],
    "paths": {
        "/captcha": {
            "get": {
                "tags": [
                    "验证码"
                ],
                "summary": "图片验证码",
                "operationId": "a785e139239a44e77684f7d505ab478f",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CaptchaResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/region": {
            "get": {
                "tags": [
                    "地区"
                ],
                "summary": "查询地区列表",
                "operationId": "7ad4dc59cc0eefa982472b2ed5423756",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "上级地区ID，默认值为0显示省份数据",
                        "required": true,
                        "example": 0
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RegionResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/sms": {
            "post": {
                "tags": [
                    "短信"
                ],
                "summary": "发送手机短信验证码",
                "operationId": "f4cc2832be956cb7e6391195d27995b0",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/SmsSendRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/upload": {
            "post": {
                "tags": [
                    "素材"
                ],
                "summary": "附件上传接口",
                "operationId": "d1ac3d195ec90e4f464849eeef6aa045",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/UploadRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UploadResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "RegionRequest": {
                "properties": {
                    "id": {
                        "description": "地区ID",
                        "type": "integer"
                    }
                },
                "type": "object"
            },
            "SmsSendRequest": {
                "required": [
                    "mobile",
                    "captcha",
                    "uuid"
                ],
                "properties": {
                    "mobile": {
                        "description": "手机号码",
                        "type": "string",
                        "example": "13901889999"
                    },
                    "captcha": {
                        "description": "图片验证码",
                        "type": "string",
                        "example": "000000"
                    },
                    "uuid": {
                        "description": "图片验证码UUID",
                        "type": "string",
                        "example": "123456"
                    }
                },
                "type": "object"
            },
            "UploadRequest": {
                "required": [
                    "file"
                ],
                "properties": {
                    "file": {
                        "description": "文件",
                        "type": "file",
                        "format": "binary"
                    }
                },
                "type": "object"
            },
            "CaptchaResponse": {
                "properties": {
                    "captcha": {
                        "description": "图片验证码",
                        "type": "string",
                        "example": "123456"
                    },
                    "uuid": {
                        "description": "验证码UUID",
                        "type": "string",
                        "example": "123456"
                    }
                },
                "type": "object"
            },
            "RegionResponse": {
                "properties": {
                    "id": {
                        "description": "地区ID",
                        "type": "integer"
                    },
                    "name": {
                        "description": "地区名称",
                        "type": "string"
                    },
                    "first_letter": {
                        "description": "地区名称首字母",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "UploadResponse": {
                "properties": {
                    "url": {
                        "description": "素材URL地址",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "OptionResponse": {
                "properties": {
                    "name": {
                        "description": "名称",
                        "type": "string",
                        "example": "name"
                    },
                    "val": {
                        "description": "值",
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "PaginateLinkVo": {
                "properties": {
                    "url": {
                        "description": "链接URL",
                        "type": "string"
                    },
                    "label": {
                        "description": "页标签",
                        "type": "string"
                    },
                    "next": {
                        "description": "当前页",
                        "type": "boolean"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "验证码",
            "description": "验证码"
        },
        {
            "name": "地区",
            "description": "地区"
        },
        {
            "name": "短信",
            "description": "短信"
        },
        {
            "name": "素材",
            "description": "素材"
        }
    ]
}